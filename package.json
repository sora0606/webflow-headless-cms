{
  "name": "uniel.jp",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "backend/server.js",
  "scripts": {
    "build": "astro build",
    "dev": "concurrently --raw \"npm:server\" \"npm:dev:*\"",
    "dev:astro": "astro dev --host",
    "dev:css": "chokidar \"src/styles/**/*.css\" --command \"touch tailwind.config.js\" --debounce 0 --silent",
    "server": "npm start --workspace=backend",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "astro sync && concurrently --raw \"npm:lint:*\"",
    "lint:astro": "astro check",
    "lint:eslint": "eslint --max-warnings 0",
    "lint:prettier": "prettier . --check",
    "lint:types": "tsc -b",
    "preview": "astro preview",
    "test": "vitest --passWithNoTests"
  },
  "dependencies": {
    "@alpinejs/collapse": "3.14.3",
    "@alpinejs/focus": "3.14.3",
    "@alpinejs/ui": "3.14.3-beta.0",
    "@astrojs/node": "8.3.4",
    "@astrojs/react": "3.6.2",
    "@astrojs/sitemap": "3.1.6",
    "@astrojs/tailwind": "5.1.1",
    "@tailwindcss/container-queries": "0.1.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "alpinejs": "3.14.1",
    "astro": "4.15.8",
    "astro-icon": "1.1.1",
    "backend": "0.0.0",
    "postcss-fluid-sizing-function": "0.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss-animate": "1.0.7",
    "tiny-invariant": "1.3.3"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.3",
    "@eslint/js": "9.11.0",
    "@types/alpinejs": "3.13.10",
    "chokidar-cli": "3.0.0",
    "concurrently": "9.0.1",
    "dotenv": "16.4.5",
    "eslint": "9.11.0",
    "eslint-plugin-astro": "1.2.4",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-tailwindcss": "3.17.4",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-tailwindcss": "0.6.8",
    "typescript-eslint": "8.6.0",
    "vitest": "2.1.1"
  },
  "engines": {
    "node": "^20.9"
  },
  "workspaces": [
    "backend"
  ]
}
